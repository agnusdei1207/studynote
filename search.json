---
layout: none
---
[
  {% assign all_docs = site.html_pages | concat: site.documents %}
  {% for doc in all_docs %}
    {% if doc.title and doc.title != '' %}
    {
      "title":    {{ doc.title | jsonify }},
      "url":      {{ doc.url | relative_url | jsonify }},
      "category": {{ doc.collection | default: "" | jsonify }},
      "body":     {{ doc.content | strip_html | jsonify }}
    }{% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]
